{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "plugins": [
    "import",
    "flowtype"
  ],
  "extends": [
    "react-app",
    "plugin:prettier/recommended",
    "prettier/flowtype"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    // "no-console": "warn",
    // "no-eval": "error",
    "import/first": "error"
  },
  "parser": "babel-eslint"
}